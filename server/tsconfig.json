{
  "compilerOptions": {
    "target": "ES2020",
    "rootDir": "src",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "strict": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
    //  "types": ["jest", "node"],
  },
  "include": ["src/**/*", "__mocks__"],
  "exclude": ["node_modules", "dist"]
}
